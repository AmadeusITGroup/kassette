{
  "name": "@amadeus-it-group/kassette",
  "version": "1.7.0",
  "description": "Development server, used mainly for testing, which proxies requests and is able to easily manage local mocks.",
  "scripts": {
    "prepare": "husky install && npm run build && npm run apidoc",
    "lint": "eslint \"packages/**/*.ts\"",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "npm run ut",
    "ut": "jest",
    "ut:watch": "jest --watch",
    "ut:coverage": "jest --coverage",
    "build": "rimraf ./dist ./build && rollup --failAfterWarnings -c ./packages/rollup.config.js && tsc -p ./packages/tsconfig.dts.json && api-extractor run --config ./packages/api-extractor.json --verbose",
    "build:coverage": "rimraf ./dist && rollup --failAfterWarnings -c ./packages/rollup.cov.config.js",
    "apidoc": "typedoc",
    "e2e": "npm run build && mocha packages/e2e/run.js",
    "e2e:coverage": "npm run build:coverage && nyc mocha packages/e2e/run.js"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "bin": "./bin/index.js",
  "keywords": [
    "record",
    "replay",
    "test",
    "testing",
    "server",
    "mock",
    "mocking",
    "har",
    "http",
    "http2",
    "https",
    "proxy",
    "proxy-server",
    "http-proxy",
    "https-proxy",
    "man-in-the-middle",
    "mitm",
    "mock-server",
    "mocking-server",
    "local-mocks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AmadeusITGroup/kassette"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-syntax-decorators": "^7.22.10",
    "@babel/plugin-syntax-typescript": "^7.22.5",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@microsoft/api-extractor": "^7.38.1",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.7",
    "@types/koa": "^2.13.10",
    "@types/koa-router": "^7.4.6",
    "@types/mime-types": "^2.1.3",
    "@types/node": "^20.8.10",
    "@types/node-forge": "^1.3.8",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.10",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "koa": "^2.14.2",
    "koa-router": "^12.0.1",
    "memfs": "^4.6.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "playwright": "^1.39.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "http2-wrapper": "^2.2.0",
    "interpret": "^3.1.1",
    "istextorbinary": "^6.0.0",
    "mime-types": "^2.1.35",
    "node-forge": "^1.3.1",
    "picocolors": "^1.0.0",
    "rechoir": "^0.8.0",
    "yaml": "^2.3.3",
    "yargs": "^17.7.2"
  },
  "files": [
    "dist",
    "bin"
  ]
}
